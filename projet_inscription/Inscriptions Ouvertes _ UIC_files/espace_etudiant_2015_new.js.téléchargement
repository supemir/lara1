        if ($('#edit-pays-preinscription').val() === 'Maroc' && current_tel[0] !== '5' && current_tel[0] !== '6' && current_tel[0] !== '7') {
            $('body').append('<div class="formError edit-gsmformError" style="top: 330.75px; left: 1061.5px; margin-top: 0px; opacity: 0.87;"><div class="formErrorContent">* Numéro de téléphone invalide <br>* Ne pas mettre 0 en début du numéro<br></div><div class="formErrorArrow"><div class="line10"><!-- --></div><div class="line9"><!-- --></div><div class="line8"><!-- --></div><div class="line7"><!-- --></div><div class="line6"><!-- --></div><div class="line5"><!-- --></div><div class="line4"><!-- --></div><div class="line3"><!-- --></div><div class="line2"><!-- --></div><div class="line1"><!-- --></div></div></div>');
            return false;
        }
