var lycees;
var lyceesprives;
var Typelycee;
$(document).ready(function () {
    $('.body-page-preinscription-mobile #edit-pays-preinscription').change(function () {


        $('#edit-gsm').removeClass('validate-8-8');
        $('#edit-gsm').removeClass('validate-9-9');
        $('#edit-gsm').removeClass('validate-9-10');
        var current_pays = $(this).val();
        if (current_pays === 'Maroc') {
            $('#edit-gsm').removeClass('validate-8-8');
            $('#edit-gsm').removeClass('validate-9-10');
            $('#edit-gsm').removeClass('validate-9-9');
            $('#edit-gsm').addClass('validate-9-9');
        } else if (current_pays === 'France') {
            $('#edit-gsm').removeClass('validate-9-10');
            $('#edit-gsm').removeClass('validate-9-9');
            $('#edit-gsm').removeClass('validate-8-8');
            $('#edit-gsm').addClass('validate-9-10');
        } else if ($.inArray(current_pays, ['Tchad', 'Zimbabwe', 'Zambie', 'Togo', 'Somalie', 'Sénégal', 'Rwanda', 'Malawi', 'Niger', 'Nigéria', 'Maurice', 'Guinée', 'Guyana', 'Ghana', 'Gambie', 'Gabon', 'Côte-dIvoire', 'Congo', 'Cap-Vert', 'Afrique-du-Sud', 'Cameroun', 'Botswana', 'Bénin', 'Angola']) !== -1) {
            $('#edit-gsm').removeClass('validate-9-10');
            $('#edit-gsm').removeClass('validate-9-9');
            $('#edit-gsm').removeClass('validate-8-8');
            $('#edit-gsm').addClass('validate-9-10');
        } else {
            $('#edit-gsm').removeClass('validate-9-10');
            $('#edit-gsm').removeClass('validate-8-8');
            $('#edit-gsm').removeClass('validate-9-9');
            $('#edit-gsm').addClass('validate-9-10');
        }
    });
    $('.body-page-preinscription-mobile #uic-forms-preinscription-candidat-new .form-submit').click(function () {


        var error = 0;
        //        -----------------Recuperation du type de formation pour executer le tag correspondant------------------------
        var current_type = '';
        if ($('#edit-type-inscription-oldBachelier').length > 0) {
            if ($('#edit-type-inscription-oldBachelier').is(':checked')) {
                current_type = $('#edit-type-inscription-oldBachelier').val();
            } else {
                current_type = $('#edit-type-inscription-bachelier').val();
            }
        } else {
            current_type = 'executive';
        }

//        validation date de naissance
        if ($('#edit-datenaissance').length > 0) {
            var dateNaiss = $('input#edit-datenaissance').val();
            $('input#edit-datenaissance').removeClass('erreur-form');
            if (dateNaiss.length > 0) {
                var dates = dateNaiss.split('/');
                var fullDate = new Date();
                if (dates[2] <= 1970 || dates[2] >= fullDate.getFullYear() - 10) {
                    $('input#edit-datenaissance').addClass('erreur-form');
                    error++;
                }
                if (dates[1] > 12 || dates[1] < 1) {
                    $('input#edit-datenaissance').addClass('erreur-form');
                    error++;
                }
                if (dates[0] > 31 || dates[0] < 1) {
                    $('input#edit-datenaissance').addClass('erreur-form');
                    error++;
                }
            } else {
                $('input#edit-datenaissance').addClass('erreur-form');
                error++;
            }
        }
//        validation de nom
        var nom = $('#edit-nom').val();
        $('#edit-nom').removeClass('erreur-form');
        if (nom.length === 0) {
            $('#edit-nom').addClass('erreur-form');
            error++;
        }
//        validation de prénom
        var prenom = $('#edit-prenom').val();
        $('#edit-prenom').removeClass('erreur-form');
        if (prenom.length === 0) {
            $('#edit-prenom').addClass('erreur-form');
            error++;
        }
//        validation de l'email
        var email = $('#edit-email').val();
        $('#edit-email').removeClass('erreur-form');
        if (!isValidEmailAddress(email)) {
            $('#edit-email').addClass('erreur-form');
            error++;
        }
//        validation du pays
        var pays = $('#edit-pays-preinscription').val();
        $('#edit-pays-preinscription').removeClass('erreur-form');
        if (pays.length === 0) {
            $('#edit-pays-preinscription').addClass('erreur-form');
            error++;
        }
//        validation du gsm

        $('#edit-gsm').removeClass('erreur-form');
        var gsm = $('#edit-gsm').val();
        if (gsm.length === 0) {
            $('#edit-gsm').addClass('erreur-form');
            error++;
        } else {
            if ($('#edit-gsm').hasClass("validate-9-9") && $('#edit-gsm').val().length != 9)
            {
                $('#edit-gsm').addClass('erreur-form');
                error++;
            } else if ($('#edit-gsm').hasClass("validate-8-8") && $('#edit-gsm').val().length != 8)
            {
                $('#edit-gsm').addClass('erreur-form');
                error++;
            } else if ($('#edit-gsm').hasClass("validate-9-10") && ($('#edit-gsm').val().length != 9 && $('#edit-gsm').val().length != 10))
            {
                $('#edit-gsm').addClass('erreur-form');
                error++;
            }
        }

        //        validation de niveau etude
        var ne = $('#edit-niveau-etude').val();
        $('#edit-niveau-etude').removeClass('erreur-form');
        if (ne.length === 0) {
            $('#edit-niveau-etude').addClass('erreur-form');
            error++;
        }

        //        validation de intereser par
        var inp = $('#edit-new-produit-demande').val();
        $('#edit-new-produit-demande').removeClass('erreur-form');
        if (inp.length === 0) {
            $('#edit-new-produit-demande').addClass('erreur-form');
            error++;
        }

        //        validation de test admission
        //var tstadmi = $('#edit-test-admission').val();
        //$('#edit-test-admission').removeClass('erreur-form');
        //if (tstadmi.length === 0) {
        //    $('#edit-test-admission').addClass('erreur-form');
        //    error++;
        //}

        //        validation de ville lycee
        if(pays == 'Maroc' && window.location.href.indexOf("formation") > -1 || window.location.href.indexOf("journee-portes-ouvertes-24-mars") > -1){
            var villelycee = $('#edit-ville').val();
            $('#edit-ville').removeClass('erreur-form');
            if (villelycee.length == 0) {
                $('#edit-ville').addClass('erreur-form');
                error++;
            }

            //        validation de nom lycee
            var nomlycee = $('#edit-lycee-bac').val();
            $('#edit-lycee-bac').removeClass('erreur-form');
            if (nomlycee.length == 0) {
                $('#edit-lycee-bac').addClass('erreur-form');
                error++;
            }

            //        validation de nom lycee
            var villepays = $('#edit-ville-pays').val();
            $('#edit-ville-pays').removeClass('erreur-form');
            if (villepays.length == 0) {
                $('#edit-ville-pays').addClass('erreur-form');
                error++;
            }
        }
        //validation civilte
        var civilite = $('input[name="civilite"]').is(':checked');
        $('.form-item-civilite-preinscription .form-item').removeClass('erreur-form');
        if(!civilite) {
            $('.form-item-civilite-preinscription .form-item').addClass('erreur-form');
            error++;
        }

        //validation accepter
        var accepter = $('input[name="accepter"]').is(':checked');
        $('#edit-accepter-wrapper').removeClass('erreur-form');
        if(!accepter) {
            $('#edit-accepter-wrapper').addClass('erreur-form');
            error++;
        }
console.log(error);
        /*   $('#edit-ville-pays').removeClass('erreur-form');
         if(window.location.href.indexOf("formation") > -1){
         $('#edit-ville').removeClass('erreur-form');
         $('#edit-lycee-bac').removeClass('erreur-form');
         }*/
        /*$('select[name="pays-preinscription"]').change(function () {
         $('#edit-ville-pays').removeClass('erreur-form');

         if(window.location.href.indexOf("formation") > -1){
         $('#edit-ville').removeClass('erreur-form');
         $('#edit-lycee-bac').removeClass('erreur-form');
         }
         if ($(this).val() == "Maroc") {
         $('#edit-ville-pays').addClass('erreur-form');

         if(window.location.href.indexOf("formation") > -1){
         $('#edit-ville').addClass('erreur-form');
         $('#edit-lycee-bac').addClass('erreur-form');
         }
         }
         });*/
