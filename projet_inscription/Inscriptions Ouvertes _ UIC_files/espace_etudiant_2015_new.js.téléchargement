        if ($("#uic-forms-preinscription-candidat-new").validationEngine('validate')) {
            var profil_web = '';
            if (href.indexOf("executive") > -1) {
                if ((jQuery.inArray($('#edit-ville-pays').val(), ['Casablanca', 'Mohammedia']) != '-1') && (jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +4', 'Bac +5']) != '-1'))
                    profil_web = (jQuery.inArray($('#edit-nb-anne-exper').val(), ['De 5 à  8 ans', 'plus de 8']) != '-1') ? 'FE_P1' : 'FE_P3';
                else if ((jQuery.inArray($('#edit-ville-pays').val(), ['Casablanca', 'Mohammedia']) != '-1') && $('#edit-niveau-etude').val() == 'Bac +3')
                    profil_web = (jQuery.inArray($('#edit-nb-anne-exper').val(), ['De 3 à  5 ans', 'De 5 à  8 ans', 'plus de 8']) != '-1') ? 'FE_P2' : 'FE_P4';
                else if ((jQuery.inArray($('#edit-ville-pays').val(), ['Casablanca', 'Mohammedia']) != '-1') && $('#edit-niveau-etude').val() == 'Bac +2')
                    profil_web = (jQuery.inArray($('#edit-nb-anne-exper').val(), ['De 5 à  8 ans', 'plus de 8']) != '-1') ? 'FE_P5' : 'FE_P6';
                else if (jQuery.inArray($('#edit-ville-pays').val(), ['Casablanca', 'Mohammedia']) == '-1')
                    profil_web = 'FE_P6';
                if (profil_web != '')
                    dataLayer.push({'event': profil_web});
            } else if (href.indexOf("formation") > -1) {

                var casaregion = ['Casablanca', 'Mohammedia', 'Settat', 'Berrechid', 'Rabat', 'El jadida', 'Kenitra', 'Temara', 'Skhirat', 'El Gara', 'Ben slimane', 'Sale', 'Deroua', 'Bouznika', 'El brouj', 'Harhoura'];

                if ((jQuery.inArray($('#edit-niveau-etude').val(), ['2ème année bac marocain', 'Déjà bachelier']) != '-1') && (jQuery.inArray($('#edit-ville').val(), casaregion) != '-1')){
                    if(jQuery.inArray($('#edit-lycee-bac').val(), ['ELBILIA', 'Lyautey', 'ALMADINA', 'LE PONT', 'EL MANAL', 'LA RESIDENCE', 'ENNOUR', 'YASSAMINE',
                            'RAMEAU', 'CALIFORNIE', 'EIC (ORT)', 'Groupe Scolaire ANFA (Bennis)', 'LOUIS MASSIGNON', 'AL JABR', 'Groupe Scolaire TAZI II', 'Groupe scolaire Rahmouni potentiel',
                            'Louis Bertrand', 'Groupe scolaire berrada', 'Groupe scolaire riad', 'CARREFOUR', 'ROMANDIE', 'MAIMONIDE', 'prépas Classes Préparatoires galilée',
                            'Lycée Pythagore']) != '-1')
                        profil_web =  'FI_B_P1';
                    else{
                        profil_web = 'FI_B_P4';
                        $.each(lyceesprives, function (index, value) {
                            if($('#edit-lycee-bac').val() === value)
                                profil_web = 'FI_B_P2';
                        });
                    }
                }else if ((jQuery.inArray($('#edit-niveau-etude').val(), ['2ème année bac marocain', 'Déjà bachelier']) != '-1') && (jQuery.inArray($('#edit-ville').val(), casaregion) == '-1') && $('#edit-lycee-bac').val() != ''){
                    profil_web = 'FI_B_P5';
                    $.each(lyceesprives, function (index, value) {
                        if($('#edit-lycee-bac').val() === value)
                            profil_web = 'FI_B_P3';
                    });
                }else if ((jQuery.inArray($('#edit-niveau-etude').val(), ['2ème année bac marocain', 'Déjà bachelier']) != '-1') && ($('#edit-pays-preinscription').val() != 'Maroc'))
                    profil_web = 'Etranger_B';
                else if ((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +1', 'Bac +2', 'Bac +3', 'Bac +4', 'Bac +5']) != '-1') && (jQuery.inArray($('#edit-ville').val(), casaregion) != '-1')){
                    if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +1', 'Bac +2', 'Bac +3']) != '-1'))
                        profil_web = 'FI_AP_P1';
                    else if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +4']) != '-1'))
                        profil_web = 'FI_AP_P3';
                    else if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +5']) != '-1'))
                        profil_web = 'FI_AP_P4';
                }else if ((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +1', 'Bac +2', 'Bac +3', 'Bac +4', 'Bac +5']) != '-1') && (jQuery.inArray($('#edit-ville').val(), casaregion) == '-1') && $('#edit-pays-preinscription').val() == 'Maroc' ){
                    if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +1', 'Bac +2', 'Bac +3']) != '-1'))
                        profil_web = 'FI_AP_P2';
                    else if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +4']) != '-1'))
                        profil_web = 'FI_AP_P3';
                    else if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +5']) != '-1'))
                        profil_web = 'FI_AP_P4';
                }else if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +1', 'Bac +2', 'Bac +3', 'Bac +4', 'Bac +5']) != '-1') && $('#edit-pays-preinscription').val() != 'Maroc')
                    profil_web = 'Etranger_AP';

                if($('input[name=type_lycee]:checked').val() != undefined){
                    if($('input[name=type_lycee]:checked').val() == 'Lycée Public')
                        profil_web = 'Etrangers_Public';
                    else
                        profil_web = 'Etrangers_Privé';
                }



                if($('#edit-pays-preinscription').val() === 'Maroc'){
                    $.ajax({
                        dataType: "html",
                        url: "/get-type-by-lycee/"+$('#edit-lycee-bac').val()+"/"+$('#edit-ville').val(),
                        success: function(data) {
                            Typelycee = (data.split('"').join('')).replace(/(\r\n|\n|\r)/gm,"");
                            console.log(Typelycee);
                            if( Typelycee == 'Privé') {
                                if(jQuery.inArray($('#edit-ville').val(), ['El Jadida', 'Rabat', 'Settat'])){
                                    profil_web = 'FI_9rayti';
                                    console.log("aaaaaaaaaaaaaaaaaaaaaaaaaaaa" + Typelycee);
                                }
                            }


                            if (profil_web != '')
                                dataLayer.push({'event': profil_web});
                            console.log(profil_web);
                        }
                    });
                }


            }

            if (href.indexOf("executive") > -1) {
                dataLayer.push({'event': 'FE_All'});
                dataLayer.push({'event': 'formation_executive'});
            } else if (href.indexOf("formation") > -1) {
                dataLayer.push({'event': 'FI_All'});
            }
            if (candidat_form_submited == false){
                candidat_form_submited = true;
            }else{
                $('.body-page-preinscription-desktop #uic-forms-preinscription-candidat-new .form-submit, .body-page-preinscription-mobile #uic-forms-preinscription-candidat-new .form-submit').attr('disabled', 'disabled');
            }

            if (window.location.href.indexOf("journee-portes-ouvertes-24-mars") > -1){
                dataLayer.push({'event': 'jpo0517'});
            }

        }
    });
    $('#edit-ville-pays-wrapper').hide();
    $('#edit-ville-pays').removeClass('validate[required]');
    if(window.location.href.indexOf("formation") > -1 || window.location.href.indexOf("journee-portes-ouvertes-24-mars") > -1){
        $('#edit-ville-wrapper').hide();
        $('#edit-lycee-bac-wrapper').hide();
        $('.form-item-type-lycee').hide();
        $('#edit-ville').removeClass('validate[required]');
        $('#edit-lycee-bac').removeClass('validate[required]');
    }
    $('select[name="pays-preinscription"]').change(function () {
        $('#edit-ville-pays-wrapper').hide();
        $('#edit-ville-pays').removeClass('validate[required]');

        if(window.location.href.indexOf("formation") > -1 || window.location.href.indexOf("journee-portes-ouvertes-24-mars") > -1){
            $('#edit-ville-wrapper').hide();
            $('#edit-lycee-bac-wrapper').hide();
            $('.form-item-type-lycee').hide();
