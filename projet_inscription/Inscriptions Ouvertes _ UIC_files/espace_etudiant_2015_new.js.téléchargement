        if (error > 0) {
            return false;
        } else {
            var href = window.location.href;
            var profil_web = '';
            if (href.indexOf("executive") > -1) {
                console.log("executive");
                if ((jQuery.inArray($('#edit-ville-pays').val(), ['Casablanca', 'Mohammedia']) != '-1') && (jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +4', 'Bac +5']) != '-1'))
                    profil_web = (jQuery.inArray($('#edit-nb-anne-exper').val(), ['De 5 à  8 ans', 'plus de 8']) != '-1') ? 'FE_P1' : 'FE_P3';
                else if ((jQuery.inArray($('#edit-ville-pays').val(), ['Casablanca', 'Mohammedia']) != '-1') && $('#edit-niveau-etude').val() == 'Bac +3')
                    profil_web = (jQuery.inArray($('#edit-nb-anne-exper').val(), ['De 3 à  5 ans', 'De 5 à  8 ans', 'plus de 8']) != '-1') ? 'FE_P2' : 'FE_P4';
                else if ((jQuery.inArray($('#edit-ville-pays').val(), ['Casablanca', 'Mohammedia']) != '-1') && $('#edit-niveau-etude').val() == 'Bac +2')
                    profil_web = (jQuery.inArray($('#edit-nb-anne-exper').val(), ['De 5 à  8 ans', 'plus de 8']) != '-1') ? 'FE_P5' : 'FE_P6';
                else if (jQuery.inArray($('#edit-ville-pays').val(), ['Casablanca', 'Mohammedia']) == '-1')
                    profil_web = 'FE_P6';
                if (profil_web != '')
                    dataLayer.push({'event': profil_web});
            } else if (href.indexOf("formation") > -1) {
                var casaregion = ['Casablanca', 'Mohammedia', 'Settat', 'Berrechid', 'Rabat', 'El jadida', 'Kenitra', 'Temara', 'Skhirat', 'El Gara', 'Ben slimane', 'Sale', 'Deroua', 'Bouznika', 'El brouj', 'Harhoura'];

                if ((jQuery.inArray($('#edit-niveau-etude').val(), ['2ème année bac marocain', 'Déjà bachelier']) != '-1') && (jQuery.inArray($('#edit-ville').val(), casaregion) != '-1')){
                    if(jQuery.inArray($('#edit-lycee-bac').val(), ['ELBILIA', 'Lyautey', 'ALMADINA', 'LE PONT', 'EL MANAL', 'LA RESIDENCE', 'ENNOUR', 'YASSAMINE',
                            'RAMEAU', 'CALIFORNIE', 'EIC (ORT)', 'Groupe Scolaire ANFA (Bennis)', 'LOUIS MASSIGNON', 'AL JABR', 'Groupe Scolaire TAZI II', 'Groupe scolaire Rahmouni potentiel',
                            'Louis Bertrand', 'Groupe scolaire berrada', 'Groupe scolaire riad', 'CARREFOUR', 'ROMANDIE', 'MAIMONIDE', 'prépas Classes Préparatoires galilée',
                            'Lycée Pythagore']) != '-1')
                        profil_web =  'FI_B_P1';
                    else{
                        profil_web = 'FI_B_P4';
                        $.each(lyceesprives, function (index, value) {
                            if($('#edit-lycee-bac').val() === value)
                                profil_web = 'FI_B_P2';
                        });
                    }
                }else if ((jQuery.inArray($('#edit-niveau-etude').val(), ['2ème année bac marocain', 'Déjà bachelier']) != '-1') && (jQuery.inArray($('#edit-ville').val(), casaregion) == '-1')){
                    profil_web = 'FI_B_P5';
                    $.each(lyceesprives, function (index, value) {
                        if($('#edit-lycee-bac').val() === value)
                            profil_web = 'FI_B_P3';
                    });
                }else if ((jQuery.inArray($('#edit-niveau-etude').val(), ['2ème année bac marocain', 'Déjà bachelier']) != '-1') && ($('#edit-pays-preinscription').val() != 'Maroc'))
                    profil_web = 'Etranger_B';
                else if ((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +1', 'Bac +2', 'Bac +3', 'Bac +4', 'Bac +5']) != '-1') && (jQuery.inArray($('#edit-ville').val(), casaregion) != '-1')){
                    if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +1', 'Bac +2', 'Bac +3']) != '-1'))
                        profil_web = 'FI_AP_P1';
                    else if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +4']) != '-1'))
                        profil_web = 'FI_AP_P3';
                    else if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +5']) != '-1'))
                        profil_web = 'FI_AP_P4';
                }else if ((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +1', 'Bac +2', 'Bac +3', 'Bac +4', 'Bac +5']) != '-1') && (jQuery.inArray($('#edit-ville').val(), casaregion) == '-1')){
                    if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +1', 'Bac +2', 'Bac +3']) != '-1'))
                        profil_web = 'FI_AP_P2';
                    else if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +4']) != '-1'))
                        profil_web = 'FI_AP_P3';
                    else if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +5']) != '-1'))
                        profil_web = 'FI_AP_P4';
                }else if((jQuery.inArray($('#edit-niveau-etude').val(), ['Bac +1', 'Bac +2', 'Bac +3', 'Bac +4', 'Bac +5']) != '-1') && $('#edit-pays-preinscription').val() != 'Maroc')
                    profil_web = 'Etranger_AP';
                if (profil_web != '')
                    dataLayer.push({'event': profil_web});
            }

            if (href.indexOf("executive") > -1) {
                dataLayer.push({'event': 'FE_All'});
                dataLayer.push({'event': 'formation_executive'});
            } else if (href.indexOf("formation") > -1) {
                dataLayer.push({'event': 'FI_All'});
            }
            console.log(profil_web);
        }
    });
    $('.body-page-preinscription-mobile #uic-forms-inscriptions-krayti .form-submit').click(function () {
        var error = 0;
        //        validation de nom
        var nom = $('#edit-nom').val();
        $('#edit-nom').removeClass('erreur-form');
        if (nom.length === 0) {
            $('#edit-nom').addClass('erreur-form');
            error++;
        }
//        validation de prénom
        var prenom = $('#edit-prenom').val();
        $('#edit-prenom').removeClass('erreur-form');
        if (prenom.length === 0) {
            $('#edit-prenom').addClass('erreur-form');
            error++;
        }
//        validation de l'email
        var email = $('#edit-email').val();
        $('#edit-email').removeClass('erreur-form');
        if (!isValidEmailAddress(email)) {
            $('#edit-email').addClass('erreur-form');
            error++;
        }

//        validation du gsm
        var gsm = $('#edit-gsm').val();
        $('#edit-gsm').removeClass('erreur-form');
        if (!isValidPhone(gsm)) {
            $('#edit-gsm').addClass('erreur-form');
            error++;
        }
//        validation du ville
        var ville = $('#edit-ville-pays').val();
        $('#edit-ville-pays').removeClass('erreur-form');
        if (ville.length === 0) {
            $('#edit-ville-pays').addClass('erreur-form');
            error++;
        }

//        validation du champ vous etes
        var typeInscription = $('#edit-type-inscription').val();
        $('#edit-type-inscription').removeClass('erreur-form');
        if (typeInscription.length === 0) {
            $('#edit-type-inscription').addClass('erreur-form');
            error++;
        }

//        validation du type bac
        var typeBac = $('#edit-Type-baccalaureat').val();
        $('#edit-Type-baccalaureat').removeClass('erreur-form');
        if (typeBac.length === 0) {
            $('#edit-Type-baccalaureat').addClass('erreur-form');
            error++;
        }
//        validation du niveau bac
        var niveauBac = $('#edit-overlay-niveau').val();
        $('#edit-Niveau-baccalaureat').removeClass('erreur-form');
        if (niveauBac.length === 0) {
            $('#edit-Niveau-baccalaureat').addClass('erreur-form');
            error++;
        }

        if (error > 0) {
            return false;
        }
    });
    $('#uic-forms-inscriptions-krayti .niveau_bac select').change(function ()
    {
        var niveau_bac = '';
        $('.niveau_bac .form-item').each(function () {
            if ($(this).css("display") === "block") {
                niveau_bac = $(this).find('select').val();
            }
        });
        $('#edit-overlay-niveau').val(niveau_bac);
    });
    $('.body-page-preinscription-mobile #uic-forms-inscriptions .form-submit').click(function () {
        var error = 0;
        //        validation de nom
        var nom = $('#edit-nom').val();
        $('#edit-nom').removeClass('erreur-form');
        if (nom.length === 0) {
            $('#edit-nom').addClass('erreur-form');
            error++;
        }
//        validation de prénom
        var prenom = $('#edit-prenom').val();
        $('#edit-prenom').removeClass('erreur-form');
        if (prenom.length === 0) {
            $('#edit-prenom').addClass('erreur-form');
            error++;
        }
//        validation de l'email
        var email = $('#edit-email').val();
        $('#edit-email').removeClass('erreur-form');
        if (!isValidEmailAddress(email)) {
            $('#edit-email').addClass('erreur-form');
            error++;
        }
//        validation du pays
        var pays = $('#edit-pays-preinscription').val();
        $('#edit-pays-preinscription').removeClass('erreur-form');
        if (pays.length === 0) {
            $('#edit-pays-preinscription').addClass('erreur-form');
            error++;
        }

//        validation du gsm
        var gsm = $('#edit-gsm').val();
        $('#edit-gsm').removeClass('erreur-form');
        if (!isValidPhone(gsm)) {
            $('#edit-gsm').addClass('erreur-form');
            error++;
        }
//        validation du ville
        var ville = $('#edit-ville-pays').val();
        $('#edit-ville-pays').removeClass('erreur-form');
        if (ville.length === 0) {
            $('#edit-ville-pays').addClass('erreur-form');
            error++;
        }

//        validation du champ vous etes
        var typeInscription = $('#edit-type-inscription').val();
        $('#edit-type-inscription').removeClass('erreur-form');
        if (typeInscription.length === 0) {
            $('#edit-type-inscription').addClass('erreur-form');
            error++;
        }

//        validation du type bac
        var typeBac = $('#edit-Type-baccalaureat').val();
        $('#edit-Type-baccalaureat').removeClass('erreur-form');
        if (typeBac.length === 0) {
            $('#edit-Type-baccalaureat').addClass('erreur-form');
            error++;
        }
//        validation du niveau bac
        var niveauBac = $('#edit-Niveau-baccalaureat').val();
        $('#edit-Niveau-baccalaureat').removeClass('erreur-form');
        if (niveauBac.length === 0) {
            $('#edit-Niveau-baccalaureat').addClass('erreur-form');
            error++;
        }

        if (error > 0) {
            return false;
        }
    });
