    if ($('.body-page-preinscription-desktop #uic-forms-preinscription-candidat-new').length > 0)
        //$('.body-page-preinscription-desktop #uic-forms-preinscription-candidat-new').validationEngine();
        $('.body-page-preinscription-desktop #uic-forms-preinscription-candidat-new').validationEngine({binded: false});
    //formulaire préinscreption
    $('#uic-forms-preinscription-candidat-new .niveau_bac select').change(function ()
    {
        var niveau_bac = '';
