    $('#uic-forms-preinscription-candidat-new #edit-civilite-Mlle').addClass('validate[required]');
    $('#edit-Type-baccalaureat').change(function () {
        $('.niveau_bac .form-item').hide();
        $('.niveau_bac select').removeClass('validate[required]');
        $('.niveau_bac select').removeAttr('type');
        $('.branche_bac select').removeClass('validate[required]');
        $('.branche_bac select').removeAttr('type');
        var typebac = $(this).val();
        if (typebac === 'Bac Marocain') {
            if ($('#edit-type-inscription').val() === 'oldBachelier')
            {
                $('#edit-Niveau-baccalaureat-maroc-old-wrapper').show();
                $('#edit-Niveau-baccalaureat-maroc-old').addClass('validate[required]');
                $('#edit-Niveau-baccalaureat-maroc-old').attr('type', 'text');
                $('#edit-branche-baccalaureat-maroc-old-wrapper').show();
                $('#edit-branche-baccalaureat-maroc-old').addClass('validate[required]');
                $('#edit-branche-baccalaureat-maroc-old').attr('type', 'text');
            } else {
                $('#edit-Niveau-baccalaureat-maroc-wrapper').show();
                $('#edit-Niveau-baccalaureat-maroc').addClass('validate[required]');
                $('#edit-Niveau-baccalaureat-maroc').attr('type', 'text');
                $('#edit-branche-baccalaureat-maroc-wrapper').show();
                $('#edit-branche-baccalaureat-maroc').addClass('validate[required]');
                $('#edit-branche-baccalaureat-maroc').attr('type', 'text');
            }
        } else {

            if ($('#edit-type-inscription').val() === 'oldBachelier')
            {
                $('#edit-Niveau-baccalaureat-autre-old-wrapper').show();
                $('#edit-Niveau-baccalaureat-autre-old-wrapper').show();
                $('#edit-Niveau-baccalaureat-autre-old').addClass('validate[required]');
                $('#edit-Niveau-baccalaureat-autre-old').attr('type', 'text');
            } else {
                $('#edit-Niveau-baccalaureat-autre-wrapper').show();
                $('#edit-Niveau-baccalaureat-autre-wrapper').show();
                $('#edit-Niveau-baccalaureat-autre').addClass('validate[required]');
                $('#edit-Niveau-baccalaureat-autre').attr('type', 'text');
            }
            if (typebac === 'Bac Français') {

                $('#edit-branche-baccalaureat-france-wrapper').show();
                $('#edit-branche-baccalaureat-france').addClass('validate[required]');
                $('#edit-branche-baccalaureat-france').attr('type', 'text');
            } else {
                $('#edit-branche-baccalaureat-autre-wrapper').show();
                $('#edit-branche-baccalaureat-autre').addClass('validate[required]');
                $('#edit-branche-baccalaureat-autre').attr('type', 'text');
            }
        }
    });
    //$('#uic-forms-preinscription-candidat-new #edit-submit-2').click(function(e){
