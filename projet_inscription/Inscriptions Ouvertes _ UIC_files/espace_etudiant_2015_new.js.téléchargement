            if ($(this).val() != "Maroc") {
                    $('.form-item-type-lycee').show();
                    $('.form-item-type-lycee #edit-type-lycee-Lycée-Public').addClass('validate[required]');
            }
        }
        if ($(this).val() == "Maroc") {
            $('#edit-ville-pays-wrapper').show();
            $('#edit-ville-pays').addClass('validate[required]');

            if(window.location.href.indexOf("formation") > -1 || window.location.href.indexOf("journee-portes-ouvertes-24-mars") > -1){
                $('#edit-ville-wrapper').show();
                $('#edit-lycee-bac-wrapper').show();
                $('#edit-ville').addClass('validate[required]');
                $('#edit-lycee-bac').addClass('validate[required]');
            }
        }
    });   //lycéé
    $('#uic-forms-preinscription-candidat-new #edit-ville').change(function () {
        $('#edit-lycee-bac').html($(this).getLyceeByVille());
    });

    $.fn.getLyceeByVille = function () {
        var currentVille = $(this).val();
        currentVille = currentVille.trimRight();
        var current_ville = lycees[currentVille];
        var html_lycee = "<option value=''>Nom du lycée</option><option value='Autre'>Autre</option>";
        $.each(current_ville, function (key, value) {
            if (key != 'Autre') {
                html_lycee += "<option value='" + key + "'>" + value + "</option>";
            }
        });
        return html_lycee;
    };

    $.ajax({
        dataType: "json",
        url: "/lycees-by-ville",
        success: success,
        error: callbackError
    });

    function success(data, textStatus, jqXHR) {
        lycees = data;
        $.each(lycees, function (index, value) {
            $.each(value, function (index_, value_) {
                value[index_] = value_.replace('--',"'");
            });
        });
    }

    function callbackError( jqXHR,  textStatus,  errorThrown){
        console.log(textStatus,errorThrown,jqXHR);
    }

    $.ajax({
        dataType: "json",
        url: "/lycees-prives",
        success: succes,
        error: callbackErrorr
    });

    function succes(data, textStatus, jqXHR) {
        lyceesprives = data;
        $.each(lyceesprives, function (index, value) {
            value[index] = value.replace('--',"'");
        });
    }

    function callbackErrorr( jqXHR,  textStatus,  errorThrown){
        console.log(textStatus,errorThrown,jqXHR);
    }
